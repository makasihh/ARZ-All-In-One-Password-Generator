{
  "version": 2,
  "builds": [
    {
      "src": "/",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "/src/components/PasswordGenerator.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/components/PasswordGenerator.py"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}
